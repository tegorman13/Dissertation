@article{beeversAssociationNegativeCognitive2019,
  title = {Association between Negative Cognitive Bias and Depression: {{A}} Symptom-Level Approach},
  shorttitle = {Association between Negative Cognitive Bias and Depression},
  author = {Beevers, Christopher G. and Mullarkey, Michael C. and {Dainer-Best}, Justin and Stewart, Rochelle A. and Labrada, Jocelyn and Allen, John J.B. and McGeary, John E. and Shumake, Jason},
  year = {2019},
